{
  "name": "my-component",
  "version": "0.0.0",
  "description": "A blank schematics",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "build-and-run": "tsc -p tsconfig.json && schematics .:my-component --name=my-file --dry-run=false",
    "build-and-run-dry": "tsc -p tsconfig.json && schematics .:my-component --name=my-file",
    "debug": "node --inspect-brk $(which schematics) .:my-component --name=my-file"
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^7.3.8",
    "@angular-devkit/schematics": "^7.3.8",
    "@types/jasmine": "^3.0.0",
    "@types/node": "^8.0.31",
    "jasmine": "^3.0.0",
    "typescript": "~3.2.2"
  }
}
